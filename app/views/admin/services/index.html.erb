<div class="wrapper wrapper-content">
<div class="row animated fadeInRight">

  <div class="col-md-12">
    <div class="ibox float-e-margins">
        <div class="ibox-title">
            <h5>Service Page Configuration</h5>
            <div class="ibox-tools">
                <a class="collapse-link">
                    <i class="fa fa-chevron-up"></i>
                </a>
                <a class="close-link">
                    <i class="fa fa-times"></i>
                </a>
            </div>
        </div>
        
        <% @admin_services.each do |admin_service| %>
          <div class="ibox-content">
              <div>
                  <div class="feed-activity-list">

                      <div class="feed-element">
                        <small class="text-muted">
                          Created at: <%= admin_service.created_at.strftime(" %I : %M %p - %A, %d %b ( %Y )") %><br>
                          Updated at: <%= admin_service.updated_at.strftime(" %I : %M %p - %A, %d %b ( %Y )") %>
                        </small>
                      </div>

                      <!-- <div class="feed-element">
                        <div class="media-body ">
                              <strong>Event Pic</strong> <br>
                              <div class="well">
                                <img alt="image" class="img-responsive" src="<%#= admin_event.eventpic_url(:thumb) %>">
                              </div>
                          </div>
                      </div> -->

                      <div class="feed-element">
                        <div class="media-body ">
                              <strong>Service Title</strong> <br>
                              <div class="well">
                                <%= admin_service.service_title.humanize %>
                              </div>
                          </div>
                      </div>

                      <div class="feed-element">
                          <div class="media-body ">
                              <strong>Service Caption</strong> <br>
                              <div class="well">
                                <%= admin_service.service_caption.humanize %>
                              </div>
                          </div>
                      </div>

                      <div class="feed-element">
                          <div class="media-body ">
                              <strong>Third Party Title</strong> <br>
                              <div class="well">
                                <%= admin_service.tp_title.humanize %>
                              </div>
                          </div>
                      </div>

                      <div class="feed-element">
                          <div class="media-body ">
                              <strong>Third Party Caption</strong> <br>
                              <div class="well">
                                <%= admin_service.tp_caption.humanize %>
                              </div>
                          </div>
                      </div>

                      <div class="feed-element">
                          <div class="media-body ">
                              <strong>Customer Title</strong> <br>
                              <div class="well">
                                <%= admin_service.cust_title.humanize %>
                              </div>
                          </div>
                      </div>

                      <div class="feed-element">
                          <div class="media-body ">
                              <strong>Customer Caption</strong> <br>
                              <div class="well">
                                <%= admin_service.cust_caption.humanize %>
                              </div>
                          </div>
                      </div>

                      <div class="feed-element">
                          <div class="media-body ">
                              <strong>Customer Descrption</strong> <br>
                              <div class="well">
                                <%= admin_service.cust_descrption.humanize %>
                              </div>
                          </div>
                      </div>

                      <div class="feed-element">
                          <div class="media-body ">
                              <strong>Notice Board Title</strong> <br>
                              <div class="well">
                                <%= admin_service.notice_board_title.humanize %>
                              </div>
                          </div>
                      </div>

                      <div class="feed-element">
                          <div class="media-body ">
                            <%= link_to 'Edit', edit_admin_service_path(admin_service), :class=>"btn btn-primary" %>
                            <%= link_to 'Destroy', admin_service, method: :delete, data: { confirm: 'Are you sure?' }, :class=>"btn btn-danger" %>
                          </div>
                      </div>                    
                              
                  </div>
              </div>
          </div>
        <% end %>


    </div>
  </div><!-- col-md-8 -->
</div>

</div>