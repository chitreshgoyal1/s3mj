<p id="notice"><%= notice %></p>

<h1>Listing Admin Scustservices</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Service</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_scustservices.each do |admin_scustservice| %>
      <tr>
        <td><%= admin_scustservice.title %></td>
        <td><%= admin_scustservice.description %></td>
        <td><%= admin_scustservice.service_id %></td>
        <td><%= link_to 'Show', admin_scustservice %></td>
        <td><%= link_to 'Edit', edit_admin_scustservice_path(admin_scustservice) %></td>
        <td><%= link_to 'Destroy', admin_scustservice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scustservice', new_admin_scustservice_path %>
