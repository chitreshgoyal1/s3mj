<div class="wrapper wrapper-content">
<div class="row animated fadeInRight">

  <div class="col-md-12">
    <div class="ibox float-e-margins">
        <div class="ibox-title">
            <h5>List: Contact Us Page</h5>
            <div class="ibox-tools">
                <a class="collapse-link">
                    <i class="fa fa-chevron-up"></i>
                </a>
                <a class="close-link">
                    <i class="fa fa-times"></i>
                </a>
            </div>
        </div>
        
        <% @admin_contact_infos.each do |admin_contact_info| %>
          <div class="ibox-content">
              <div>
                  <div class="feed-activity-list">

                      <div class="feed-element">
                        <small class="text-muted">
                          Created at: <%= admin_contact_info.created_at.strftime(" %I : %M %p - %A, %d %b ( %Y )") %><br>
                          Updated at: <%= admin_contact_info.updated_at.strftime(" %I : %M %p - %A, %d %b ( %Y )") %>
                        </small>
                      </div>

                      <div class="feed-element">
                        <div class="col-md-12 ">
                          <iframe src="<%= admin_contact_info.embed_url %>" width="100%" height="300" frameborder="0" style="border:0" allowfullscreen></iframe>
                        </div>
                      </div>

                      <div class="feed-element">
                          <div class="media-body ">
                              <strong>Email Address 1</strong> <br>
                              <div class="well">
                                <%= admin_contact_info.email1 %>
                              </div>
                              <div class="pull-right">
                                  <a class="btn btn-xs btn-white"><i class="fa fa-thumbs-up"></i> Like </a>
                              </div>
                          
                              <strong>Email Address 2</strong> <br>
                              <div class="well">
                                <%= admin_contact_info.email2 %>
                              </div>
                              <div class="pull-right">
                                  <a class="btn btn-xs btn-white"><i class="fa fa-thumbs-up"></i> Like </a>
                              </div>
                          </div>
                      </div>

                      <div class="feed-element">
                          <div class="media-body ">
                              <strong>Phone 1</strong> <br>
                              <div class="well">
                                <%= admin_contact_info.phone1_code %> - <%= admin_contact_info.phone1_number %>
                              </div>
                              <div class="pull-right">
                                  <a class="btn btn-xs btn-white"><i class="fa fa-thumbs-up"></i> Like </a>
                              </div>
                          
                              <strong>Phone 2</strong> <br>
                              <div class="well">
                                <%= admin_contact_info.phone2_code %> - <%= admin_contact_info.phone2_number %>
                              </div>
                              <div class="pull-right">
                                  <a class="btn btn-xs btn-white"><i class="fa fa-thumbs-up"></i> Like </a>
                              </div>
                          </div>
                      </div>
                      
                      <div class="feed-element">
                          <div class="media-body ">
                              <strong>Name, Address, City, State, Country</strong> <br>
                              <div class="well">
                                <%= admin_contact_info.name %> <br><br>
                                <strong> Address </strong><br><br>
                                <%= admin_contact_info.add_line1 %><br>
                                <%= admin_contact_info.add_line2 %><br>
                                <%= admin_contact_info.add_line3 %><br><br>
                                <%= admin_contact_info.city %>, <%= admin_contact_info.state %><br>
                                <%= admin_contact_info.country %>
                              </div>
                              <div class="pull-right">
                                  <a class="btn btn-xs btn-white"><i class="fa fa-thumbs-up"></i> Like </a>
                              </div>
                          </div>
                      </div>

                      <div class="feed-element">
                          <div class="media-body ">
                              <strong>Mobile 1</strong>
                              <div class="well">
                                <%= admin_contact_info.mobile1 %><br>
                              </div>
                              <div class="pull-right">
                                  <a class="btn btn-xs btn-white"><i class="fa fa-thumbs-up"></i> Like </a>
                              </div>
                          
                              <strong>Mobile 2</strong> <br>
                              <div class="well">
                                <%= admin_contact_info.mobile2 %><br>
                              </div>
                              <div class="pull-right">
                                  <a class="btn btn-xs btn-white"><i class="fa fa-thumbs-up"></i> Like </a>
                              </div>
                          </div>
                      </div>

                      <div class="feed-element">
                          <div class="media-body ">
                              <strong>Facebook URL:</strong> <br>
                              <div class="well">
                                <%= admin_contact_info.facebook_url %><br>
                              </div>
                              <div class="pull-right">
                                  <a class="btn btn-xs btn-white"><i class="fa fa-thumbs-up"></i> Like </a>
                              </div>
                          
                              <strong>Twitter URL</strong> <br>
                              <div class="well">
                                <%= admin_contact_info.twitter_url %><br>
                              </div>
                              <div class="pull-right">
                                  <a class="btn btn-xs btn-white"><i class="fa fa-thumbs-up"></i> Like </a>
                              </div>
                          </div>
                      </div>                    

                      <div class="feed-element">
                          <div class="media-body ">
                            <%= link_to 'Edit', edit_admin_contact_info_path(admin_contact_info), :class=>"btn btn-primary" %>
                            <%= link_to 'Destroy', admin_contact_info, method: :delete, data: { confirm: 'Are you sure?' }, :class=>"btn btn-danger" %>
                          </div>
                      </div>                    
                              
                  </div>
              </div>
          </div>
        <% end %>
    </div>
  </div><!-- col-md-8 -->
</div>

</div>