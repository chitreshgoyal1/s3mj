<p id="notice"><%= notice %></p>

<h1>Listing Admin Smedialists</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Admin service</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_smedialists.each do |admin_smedialist| %>
      <tr>
        <td><%= admin_smedialist.title %></td>
        <td><%= admin_smedialist.description %></td>
        <td><%= admin_smedialist.admin_service %></td>
        <td><%= link_to 'Show', admin_smedialist %></td>
        <td><%= link_to 'Edit', edit_admin_smedialist_path(admin_smedialist) %></td>
        <td><%= link_to 'Destroy', admin_smedialist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Smedialist', new_admin_smedialist_path %>
