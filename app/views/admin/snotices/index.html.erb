<p id="notice"><%= notice %></p>

<h1>Listing Admin Snotices</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Service</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_snotices.each do |admin_snotice| %>
      <tr>
        <td><%= admin_snotice.description %></td>
        <td><%= admin_snotice.service_id %></td>
        <td><%= link_to 'Show', admin_snotice %></td>
        <td><%= link_to 'Edit', edit_admin_snotice_path(admin_snotice) %></td>
        <td><%= link_to 'Destroy', admin_snotice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Snotice', new_admin_snotice_path %>
